Cpp code :

#include<bits/stdc++.h>
 
using namespace std;
 
int main()
{
 
int n,cnt=0;
cin>>n;
int a[n],b[n];
int m= 50001;
 
for (int i = 0; i < n; i++)
{
   cin>>a[i];
   m=min(m,a[i]);
}
 
for (int j = 0; j < n; j++)
{
  cin>>b[j];
}
 
int n_same = 1;
 
while (n_same) {
n_same = 0;
 
for (int i = 0; i < n; i++)
 {
     while (a[i] > m && a[i] != 0) {
        a[i] = a[i] - b[i];
        n_same = 1;
        cnt++;
}
 
        if (m > a[i])
        m = a[i];
 
        if (m < 0)
        break;
 
}
 
}
 
if (m < 0)
cout<<-1; 
 
else
cout<<cnt;
 
return 0;
}
