Cpp code :

#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
    ll t;
    cin>>t;
    ll n,i,j,k,l,m;
    
    while(t--)
    {
        cin>>n;
        ll a[n];
        l=INT_MAX;
        j=0;
        for(i=0;i<n;i++)
        {
            cin>>a[i];
        }
        sort(a,a+n);
        for(i=0;i<n-1;i++)
        {
            j=a[i]^a[i+1];
            l=min(l,j);
        }

        cout<<l<<endl;
    }
    return 0;
}
