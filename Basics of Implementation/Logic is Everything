	while(t--)
	{
		long long n,p,k,j,l;
		k=0;
		cin>>n;
		if(n%10==0)
		{
			cout<<"2"<<endl;
		}
 
		else
		{
			cout<<"1"<<endl;
            cout<<(n%10)<<endl;
		}
		
	}
 
